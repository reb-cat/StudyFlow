# Environment Configuration
NODE_ENV=development

# Database
DATABASE_URL=postgresql://username:password@host:port/database

# Authentication & Security
SESSION_SECRET=your-session-secret-here
FAMILY_PASSWORD=your-family-password

# Canvas Integration
CANVAS_BASE_URL=https://your-canvas-instance.com
CANVAS_BASE_URL_2=https://your-second-canvas-instance.com
ABIGAIL_CANVAS_TOKEN=your-abigail-canvas-token
ABIGAIL_CANVAS_TOKEN_2=your-abigail-second-canvas-token
KHALIL_CANVAS_TOKEN=your-khalil-canvas-token

# Email Configuration
RESEND_API_KEY=your-resend-api-key
RESEND_FROM=your-sender-email@example.com
PARENT_EMAIL=parent@example.com

# Server Configuration (automatically set by hosting platform)
PORT=5000

# Database Connection Details (usually auto-generated)
PGHOST=your-postgres-host
PGPORT=5432
PGUSER=your-postgres-user
PGPASSWORD=your-postgres-password
PGDATABASE=your-postgres-database

# Object Storage (if using Replit object storage)
DEFAULT_OBJECT_STORAGE_BUCKET_ID=your-bucket-id
PRIVATE_OBJECT_DIR=/your-private-dir
PUBLIC_OBJECT_SEARCH_PATHS=/your-public-paths

# Optional APIs
ELEVEN_LABS_API_KEY=your-eleven-labs-key
SUPABASE_ANON_KEY=your-supabase-anon-key